[user]

	email = robertphoenix@gmail.com
  	name = Rob Phoenix

[alias]

	i = status -s
	memo = commit -am
	sum = add --all
	syncm = push origin master
	syncd = push origin develop
	branches = branch -a
	tags = tag
	stashes = stash list
	unstage = reset -q HEAD --
	discard = checkout --
	uncommit = reset --mixed HEAD~
	amend = commit --amend
	unmerged = diff --name-only --diff-filter=U
	remotes = remote -v

[color]

	ui = true

[help]

	autocorrect = 1

[core]

	excludesfile = ~/.gitignore
	editor = subl -n -w

[github]

	user = bordeltabernacle

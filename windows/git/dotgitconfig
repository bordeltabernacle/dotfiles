[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
[user]
	name = Rob Phoenix
	email = rob.phoenix@bt.com
[core]
  excludesfile = ~/gitignore-global
  editor = subl -n -w
[color]
  ui = true
[help]
  autocorrect = 1
[github]
  user = bordeltabernacle
[credential]
  helper = cache --timeout=3600
[alias]
  i = status -s
  memo = commit -am
  sum = add --all
  sync = push origin master
  branches = branch -a
  tags = tag
  stashes = stash list
  unstage = reset -q HEAD --
  discard = checkout --
  uncommit = reset --mixed HEAD~
  amend = commit --amend
  unmerged = diff --name-only --diff-filter=U
  remotes = remote -v

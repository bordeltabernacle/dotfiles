# Personal aliases
alias say="sudo apt -y"
alias sarepo="sudo add-apt-repository"
alias sudo="sudo "
alias su="sudo -i"
alias s="sudo"
alias up="sudo apt -y update && sudo apt -y upgrade"
alias rd="sudo rm -r"
alias bye="sudo shutdown now"
alias reup="sudo reboot now"

alias l="ls -lAh -I.cache -I__pycache__ -I.git -Inode_modules -I.vagrant -I.DS_Store"

alias web="google-chrome"
alias open="xdg-open"
alias rng="ranger"
alias f="fuck"
alias g="git"

alias v="nvim"
alias factoryvim="vim -u ~/dotfiles/.vim/essential.vim"
alias relo="source ~/.zshrc"
alias dot="nvim ~/dotfiles"

#alias vpnup="nmcli --ask con up id 'PIA - UK London'"

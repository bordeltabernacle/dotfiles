alias p="python3"
alias bpy="bpython"
alias pyt="pytest"

alias exf="exercism fetch"
alias exs="exercism submit"

alias cscon="sudo screen /dev/ttyUSB0 9600"

alias u="usb"
alias uu="sudo umount /media/usb"

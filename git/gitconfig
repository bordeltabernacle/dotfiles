[user]

	email = robertphoenix@gmail.com
  	name = Rob Phoenix

[alias]

	i = status -s
	memo = commit -am
	sum = add --all
	sync = push origin master
	branches = branch -a
	tags = tag
	stashes = stash list
	unstage = reset -q HEAD --
	discard = checkout --
	uncommit = reset --mixed HEAD~
	amend = commit --amend
	unmerged = diff --name-only --diff-filter=U
	remotes = remote -v

[color]

	ui = true

[help]
	
	autocorrect = 1

[core]

	excludesfile = ~/gitignore-global
	editor = subl -n -w

[github]

	user = bordeltabernacle
